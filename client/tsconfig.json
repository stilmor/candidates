{
  "include": ["src/**/*.ts", "src/**/*.d.ts", "src/**/*.vue", "src/components/InterviewForm.vue.js", "src/components/InterviewForm.vue.js"],
  "compilerOptions": {
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "target": "ES2020",
    "jsx": "preserve",
    "allowJs": false,
    "strict": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "useDefineForClassFields": true,
    "lib": ["ES2020", "DOM", "DOM.Iterable"],
    "types": ["vite/client", "vuetify"],
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"]
    }
  }
}